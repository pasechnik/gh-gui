  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "react-scripts": "1.1.5",
    "redux-devtools": "^3.4.1",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start:all": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build:all": "npm-run-all build-css build-js",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "build:snapshot": "better-npm-run build:snapshot",
    "test": "better-npm-run test:ci",
    "eject": "react-scripts eject",
    "posttest": "npm run lint",
    "test:ci": "better-npm-run test:ci",
    "test:update": "better-npm-run test:ci -u",
    "test:watch": "better-npm-run test:watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test:all": "better-npm-run test:all",
    "cpcss": "cp -R node_modules/slick-carousel/slick/slick.css node_modules/slick-carousel/slick/slick-theme.css src/styles/slick/"
  },
  "betterScripts": {
    "start": {
      "command": "react-scripts start",
      "env": {
        "NODE_ENV": "development",
        "REACT_APP_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "react-scripts start",
      "env": {
        "NODE_ENV": "production",
        "REACT_APP_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "development",
        "REACT_APP_ENV": "development"
      }
    },
    "build:snapshot": {
      "command": "react-scripts build && react-snapshot",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "react-scripts test --env=jsdom",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test:ci": {
      "command": "react-scripts test --env=jsdom",
      "env": {
        "CI": true,
        "DEBUG": "app:*"
      }
    },
    "test:watch": {
      "command": "react-scripts test --env=jsdom",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pasechnik/gh-gui"
  },
  "keywords": [
    "react",
    "redux",
    "create-react-app",
    "react-loadable",
    "react-redux",
    "react-router",
    "reactstrap",
    "redux-observable",
    "boilerplate"
  ],
  "author": {
    "name": "Pasechnik Vladimir",
    "url": "https://github.com/pasechnik"
  },
  "license": "MIT"
}
